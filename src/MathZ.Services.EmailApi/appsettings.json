{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Aspire": {
        "Confluent": {
            "Kafka": {
                "Consumer": {
                    "Config": {
                        "AutoOffsetReset": "Earliest",
                        "GroupId": "aspire"
                    }
                }
            }
        }
    }
}
